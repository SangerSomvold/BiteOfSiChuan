-- ----------------------------
-- Table structure for collection
-- ----------------------------
CREATE TABLE `collection` (
  `collection_id` int(11) NOT NULL AUTO_INCREMENT,
  `user_id` varchar(50) NOT NULL DEFAULT '',
  `shop_id` int(11) DEFAULT NULL,
  PRIMARY KEY (`collection_id`),
  KEY `fk` (`user_id`),
  KEY `shop_id` (`shop_id`),
  CONSTRAINT `collection_ibfk_1` FOREIGN KEY (`shop_id`) REFERENCES `shop` (`shop_id`),
  CONSTRAINT `fk` FOREIGN KEY (`user_id`) REFERENCES `user` (`user_id`)
) ENGINE=InnoDB AUTO_INCREMENT=48 DEFAULT CHARSET=utf8;

-- ----------------------------
-- Table structure for comment
-- ----------------------------
CREATE TABLE `comment` (
  `comment_id` int(11) NOT NULL AUTO_INCREMENT,
  `shop_id` int(11) DEFAULT NULL,
  `grade` float(10,0) DEFAULT NULL,
  `user_id` varchar(50) DEFAULT '',
  `datetime` varchar(255) DEFAULT '',
  `comment` text,
  `cost` int(11) DEFAULT NULL,
  PRIMARY KEY (`comment_id`),
  KEY `shop_id` (`shop_id`),
  KEY `user_id` (`user_id`),
  CONSTRAINT `comment_ibfk_1` FOREIGN KEY (`shop_id`) REFERENCES `shop` (`shop_id`),
  CONSTRAINT `comment_ibfk_2` FOREIGN KEY (`user_id`) REFERENCES `user` (`user_id`)
) ENGINE=InnoDB AUTO_INCREMENT=64 DEFAULT CHARSET=utf8;

-- ----------------------------
-- Table structure for picture
-- ----------------------------
CREATE TABLE `picture` (
  `pic_id` int(11) NOT NULL AUTO_INCREMENT,
  `type` int(11) DEFAULT NULL,
  `pic_url` varchar(200) DEFAULT NULL,
  `id` int(11) DEFAULT NULL,
  `user_id` varchar(50) DEFAULT NULL,
  `datetime` varchar(50) DEFAULT NULL,
  PRIMARY KEY (`pic_id`),
  KEY `fk_picture_comment` (`type`)
) ENGINE=InnoDB AUTO_INCREMENT=1097 DEFAULT CHARSET=utf8;

-- ----------------------------
-- Table structure for shop
-- ----------------------------
CREATE TABLE `shop` (
  `shop_id` int(50) NOT NULL AUTO_INCREMENT,
  `user_id` varchar(50) DEFAULT '',
  `shop_name` varchar(50) DEFAULT NULL,
  `icon_url` varchar(200) DEFAULT NULL,
  `lng` double DEFAULT NULL,
  `lat` double DEFAULT NULL,
  `address` varchar(50) DEFAULT NULL,
  `feature` text,
  `grade_avg` double DEFAULT NULL,
  `cost_avg` double DEFAULT NULL,
  `type_id` int(11) DEFAULT NULL,
  PRIMARY KEY (`shop_id`),
  KEY `user_id` (`user_id`),
  KEY `type_id` (`type_id`),
  CONSTRAINT `shop_ibfk_3` FOREIGN KEY (`type_id`) REFERENCES `type` (`type_id`) ON DELETE CASCADE ON UPDATE CASCADE,
  CONSTRAINT `shop_ibfk_1` FOREIGN KEY (`user_id`) REFERENCES `user` (`user_id`)
) ENGINE=InnoDB AUTO_INCREMENT=1088 DEFAULT CHARSET=utf8;

-- ----------------------------
